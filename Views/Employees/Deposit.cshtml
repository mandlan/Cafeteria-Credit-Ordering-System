@model Cafeteria_Credit___Ordering_System.ViewModels.DepositViewModel

<h2>Deposit Funds</h2>

<form asp-action="Deposit" method="post">
    <div class="form-group">
        <label asp-for="EmployeeNumber"></label>
        <input asp-for="EmployeeNumber" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DepositAmount"></label>
        <input asp-for="DepositAmount" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Submit Deposit</button>
</form>

